orm.date\" autocomplete=\"off\" placeholder=\"输入日期\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"Name\" :label-width=\"formLabelWidth\">\r\n            <el-input v-model=\"addResForm.name\" autocomplete=\"off\" placeholder=\"输入姓名\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"Address\" :label-width=\"formLabelWidth\">\r\n            <el-input v-model=\"addResForm.address\" autocomplete=\"off\" placeholder=\"是否学习\" />\r\n          </el-form-item>\r\n        </el-form>\r\n        <template #footer>\r\n          <span class=\"dialog-footer\">\r\n              <el-button @click=\"dialogFormVisible = false\">Cancel</el-button>\r\n              <el-button @click=\"onReset\">Reset</el-button>\r\n              <el-button type=\"primary\" @click=\"dialogFormVisible = false; onSubmit(); showMessage = true\">\r\n                  Confirm\r\n              </el-button>\r\n          </span>\r\n      </template>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script setup>\r\n/* eslint-disable */\r\nimport { reactive, ref } from 'vue'\r\nimport axios from 'axios'\r\n\r\nconst dialogFormVisible = ref(false)\r\nconst formLabelWidth = '140px'\r\nconst showMessage = ref(false)\r\nlet refresh = ref(true)\r\n\r\nconst addResForm = reactive({\r\n  date: '',\r\n  name: '',\r\n  address: [],\r\n})\r\n\r\nfunction initForm() {\r\n  addResForm.date = '';\r\n  addResForm.name = '';\r\n  addResForm.address = [];\r\n}\r\n\r\n\r\nconst path = 'http://localhost:5000/resources'\r\nconst resources = ref([])\r\n\r\nfunction addRes(payload) {\r\n  axios.post(path, payload)\r\n    .then(() => {\r\n      resources.clear()\r\n      getResources()\r\n    })\r\n    .catch((error) => {\r\n      // eslint-disable-next-line\r\n      console.error(error)\r\n      getResources()\r\n    })\r\n}\r\n\r\nfunction getResources() {\r\n  axios.get(path)\r\n    .then((res) => {\r\n      console.log(res.data)\r\n      res.data.resources.forEach(element => {\r\n        resources.value.push(element)\r\n      })\r\n    })\r\n    .catch((error) => {\r\n      // eslint-disable-next-line\r\n      console.error(error)\r\n    })\r\n}\r\n\r\n\r\nfunction onSubmit(evt) {\r\n  // evt.preventDefault();\r\n  let address = false;\r\n  if (addResForm.address) address = true;\r\n  console.log(address)\r\n  const payload = {\r\n    date: addResForm.date,\r\n    name: addResForm.name,\r\n    address,\r\n  };\r\n  console.log(payload)\r\n\r\n  addRes(payload)\r\n  refresh = true\r\n  initForm()\r\n}\r\n\r\nfunction onReset(evt) {\r\n  // evt.preventDefault();\r\n  initForm()\r\n}\r\n\r\ngetResources()\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.el-button--text {\r\n  margin-right: 15px;\r\n}\r\n\r\n.el-select {\r\n  width: 300px;\r\n}\r\n\r\n.el-input {\r\n  width: 300px;\r\n}\r\n\r\n.dialog-footer button:first-child {\r\n  margin-right: 10px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}